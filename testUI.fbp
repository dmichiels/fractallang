% First two buttons
testButton(ui/button) ui_create_out -> ui_create_in#1 lr(ui/lr) ui_create_out -> ui_create_in#1 td(ui/td)
t2Button(ui/text) ui_create_out -> ui_create_in#2 lr()

% Close button
closeButton(ui/button) ui_create_out -> ui_create_in#2 td() ui_create_out -> ui_create_in win(ui/window)

% Links for unhandled events
testButton() events_out_default -> events lr() 
t2Button() events_out_default -> events lr()
lr() events_out_default -> events td()
closeButton() events_out_default -> events td()
td() events_out_default -> events win()
win() events_out_default -> input dispDefault(display)

% Intercept envents
testButton() events_out#button_clicked -> input get1(string) out -> events t2Button() events_out#get1 -> input dispEvent(display)
"opt(text:set([115 97 108 117 116]))" -> options get1()

% Some options
"opt(pre:'default ')" -> options dispDefault()
"opt(pre:'specific ')" -> options dispEvent()
"opt(text:'test 1')" -> options testButton()
"opt(text:'test 2')" -> options t2Button()
"opt(text:'close')" -> options closeButton()