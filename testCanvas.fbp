% First step
canvas(ui/canvas) ui_out -> place#1 td(ui/td) ui_out -> ui_in win(ui/window)
canvas() actions_out -> actions_in td()
td() actions_out -> input disp(display)
"set(width:500 height:500)" -> ui_in canvas()
"start" -> ui_in td()
"scan(mark 0 0)" -> actions_in canvas()
"scan(dragto 5 5)" -> actions_in canvas()
% Arc
arcItem(ui/canvas/arc) ui_out -> widget canvas()
arcItem() actions_out -> actions_in canvas()
"sstart(10 10 100 100)" -> ui_in arcItem()
% Bitmap
bitmap(ui/canvas/bitmap) ui_out -> widget canvas()
bitmap() actions_out -> actions_in canvas()
"start(10 10 bitmap:warning)" -> ui_in bitmap()
% Image
imItem(ui/canvas/image) ui_out -> widget canvas()
imItem() actions_out -> actions_in canvas()
"photo(file:"logo.gif")" -> input im(ui/image) output -> image imItem()
"start(350 150)" -> ui_in imItem()
% Line
 line(ui/canvas/line) ui_out -> widget canvas()
 line() actions_out -> actions_in canvas()
"start(0 0 200 50 50 200 200 200)" -> ui_in line()
"setCoords(0 0 100 100 0 100 200 100)" -> actions_in line()
% Oval
oval(ui/canvas/oval) ui_out -> widget canvas()
oval() actions_out -> actions_in canvas()
"start(100 100 150 150)" -> ui_in oval()
% Polygon
poly(ui/canvas/polygon) ui_out -> widget canvas()
poly() actions_out -> actions_in canvas()
"start(150 100 200 120 240 180 210 200 150 150 100 200)" -> ui_in poly()
% Rectangle
rect(ui/canvas/rectangle) ui_out -> widget canvas()
rect() actions_out -> actions_in canvas()
"getCoords(output:coordRectTest)" -> actions_in rect()
"start(10 10 100 100)" -> ui_in rect()
"move(0 100)" -> actions_in rect()
% Text
text(ui/canvas/text) ui_out -> widget canvas()
text() actions_out -> actions_in canvas()
"set(150 10 text:"hello world!")" -> ui_in text()
% Window
winItem(ui/canvas/window) ui_out -> widget canvas()
winItem() actions_out -> actions_in canvas()
"start(250 350)" -> ui_in winItem()
%"td(label(text:"haha"))" -> window winItem()
"set(text:"button")" -> ui_in b(ui/button) ui_out -> place#1 tdw(ui/td)
b2(calculator/calculator) ui_out -> place#2 tdw() ui_out -> window winItem()
b() actions_out -> actions_in tdw() actions_out -> actions_in winItem()
b2() actions_out -> input dffdsq(display) output -> actions_in tdw()
"start" -> ui_in tdw()
% Options
"opt(pre:'default ')" -> options disp()

