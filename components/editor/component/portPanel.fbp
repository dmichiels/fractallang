% ---
% Logic stream
in => in awIn(QTk/actionsWrapper)
awIn() out -> in awOut(QTk/actionsWrapper) out => out
awOut() action => action

% ---
% UI stream
awIn() action#create -> options manager(editor/component/managerPortPanel) out -> in awOut()

% ---
% Add, remove, move
awIn() action#add -> input manager()
awIn() action#addArrayPort -> input manager()
awIn() action#addinArrayPort -> input manager()
awIn() action#delete -> input manager()
awIn() action#move -> input manager()

% ---
% The move logic
awIn() action#position -> input manager()

% ---
% Hide and show logic
awIn() action#openPorts -> input manager()
awIn() action#closePorts -> input manager()
manager() border -> in awOut()

% ---
% Wrap createLink to make beginLink/endLink and good x/y
manager() action#createLink -> input createLinkLogic(editor/component/createLinkLogic) output -> in awOut()

% ---
% delete
awIn() action#delete -> input manager()