% ---
% Logic stream
in => in awIn(QTk/actionsWrapper)
awIn() out -> in awOut(QTk/actionsWrapper) out => out
awOut() action => action

% ---
% UI stream
awIn() action#create ->  in create(editor/component/createPortArray) rect -> in rect(QTk/canvas/rectangle)
rect() out -> in awOut()
create() name -> in text(QTk/canvas/text) out -> in awOut()
create() nameAtom -> options addName(editor/component/addName)
create() nameAtom -> options addNameCreate(editor/component/addNameCreate)
% ---
% Move
awIn() action#move -> in rect()
awIn() action#move -> input moveLogic(editor/component/moveLogic) output -> in awOut()
awIn() action#move -> in text()

% ---
% setCoords
awIn() action#setCoords -> input center(editor/component/center) rect -> in rect()
center() text -> in text()

% ---
% Hide and show
awIn() action#set -> in rect()
awIn() action#set -> in text()
rect() action#Enter -> in awOut()
text() action#Enter -> in awOut()

% ---
% delete
awIn() action#delete -> in rect()
awIn() action#delete -> in text()
awIn() action#delete -> in awOut()

% ---
% When create link, add information
awIn() action#createLink -> input addName() output -> in awOut()

% ---
% Double click to add an sub-port
rect() action#ButtonPress -> input dclick(mouse/doubleclick)
text() action#ButtonPress -> input dclick()
dclick() output -> input addNameCreate()
addNameCreate() output -> in awOut()

% ---
% Don't send the moveBegin and moveEnd action outside
awOut() action#moveBegin -> input discard(discard)
awOut() action#moveEnd -> input discard()