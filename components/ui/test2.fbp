% First step
canvas(ui/canvas) ui_out -> ui_in#1 td(ui/td) ui_out -> ui_in win(ui/window)
canvas() actions_out -> actions_in td()
td() actions_out -> input disp(display)
"set(width:200 height:200)" -> actions_in canvas()
% Arc
arcItem(ui/canvas/arc) ui_out -> actions_in canvas()
arcItem() actions_out -> actions_in canvas()
"setCoords(10 10 100 100)" -> actions_in arcItem()
% Bitmap
bitmap(ui/canvas/bitmap) ui_out -> actions_in canvas()
bitmap() actions_out -> actions_in canvas()
"setCoords(10 10)" -> actions_in bitmap()
"set(bitmap:error)" -> actions_in bitmap()
% Image 

% Line
line(ui/canvas/line) ui_out -> actions_in canvas()
line() actions_out -> actions_in canvas()
"setCoords(0 0 200 50 50 200 200 200)" -> actions_in line()
% Oval
oval(ui/canvas/oval) ui_out -> actions_in canvas()
oval() actions_out -> actions_in canvas()
"setCoords(100 100 150 150)" -> actions_in oval()
% Polygon
poly(ui/canvas/polygon) ui_out -> actions_in canvas()
poly() actions_out -> actions_in canvas()
"setCoords(150 100 200 120 240 180 210 200 150 150 100 200)" -> actions_in poly()
% Rectangle
rect(ui/canvas/rectangle) ui_out -> actions_in canvas()
rect() actions_out -> actions_in canvas()
"setCoords(0 150 100 190)" -> actions_in rect()
% Text
text(ui/canvas/text) ui_out -> actions_in canvas()
text() actions_out -> actions_in canvas()
"setCoords(150 10)" -> actions_in text()
"set(text:"hello world!")" -> actions_in text()
% Window
winItem(ui/canvas/window) ui_out -> actions_in canvas()
winItem() actions_out -> actions_in canvas()
"setCoords(50 150)" -> actions_in winItem()
"set(window:td(label(text:test)))" -> actions_in winItem()

% Options
"opt(pre:'default ')" -> options disp()
