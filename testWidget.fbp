td(QTk/td) out -> in w(QTk/window) out -> input d(display)
"opt(pre:default)" -> options d()
"create" -> in#td td()

b(QTk/button) out -> in#1 td()
"create(text:"Test" glue:ew)" -> in b()
b() action#button_clicked -> input d2(display)
"opt(pre:'catch')" -> options d2()


"create" -> in#lr lr(QTk/lr) out -> in#2 td()
label(QTk/label) out -> in#1 lr()
"create(init:"checkbox : ")" -> in label()
checkbutton(QTk/checkbutton) out -> in#2 lr()
"create" -> in checkbutton()

dropdownlistbox(QTk/dropdownlistbox) out -> in#3 td()
"create" -> in dropdownlistbox()
"["choice1" "choice2" "choice3"]" -> list dropdownlistbox()

grid(QTk/grid) out -> in#4 td()
"create" -> in#grid grid()
"create(label(text:"hello"))" -> in#1x1 grid()
"create(label(text:"world"))" -> in#2x2 grid()
"create(label(text:"helloworld") columnspan:2)" -> in#1x5 grid()

entry(QTk/entry) out -> in#5 td()
"create(init:"here")" -> in entry()

listbox(QTk/listbox) out -> in#6 td()
"create" -> in listbox()
"["hello" "world" "helloworld"]" -> list listbox()

message(QTk/message) out -> in#7 td()
"create(init:"this is a fractalide card" aspect:70)" -> in message()

numberentry(QTk/numberentry) out -> in#8 td()
"create(init:0 min:0 max:10)" -> in numberentry()

panel(QTk/panel) out -> in#9 td()
"create" -> in#panel panel()
"create(init:"first panel")" -> in l1(QTk/label) out -> input addT1(QTk/addTitle) output -> in#1 panel()
"opt(title:"panel 1")" -> options addT1()
"create(init:"third panel")" -> in l3(QTk/label) out -> input addT3(QTk/addTitle) output -> in#7 panel()
"opt(title:"panel 3")" -> options addT3()
"create(init:"second panel")" -> in l2(QTk/label) out -> input addT2(QTk/addTitle) output -> in#3 panel()
"opt(title:"panel 2")" -> options addT2()
b() action#button_clicked -> input ipDelete(ipEdit) out -> in#3 panel()
"opt(text:delete)" -> options ipDelete()

placeholder(QTk/placeholder) out -> in#10 td()
"create" -> in placeholder()
"create(init:"place 1")" -> in lph1(QTk/label) out -> place placeholder()
"create(init:"place 2")" -> in lph2(QTk/label) out -> place placeholder()
"display" -> in lph1()
b() action#button_clicked -> input ipDisplay(ipEdit) out -> in lph2()
"opt(text:display)" -> options ipDisplay()

"create" -> in#lr lrradio(QTk/lr) out -> in#11 td()
"create(text:"choice 1" group:r1)" -> in rb1(QTk/radiobutton) out -> in#1 lrradio()
"create(text:"choice 2" group:r1)" -> in rb2(QTk/radiobutton) out -> in#2 lrradio()
"create(text:"choice 3" group:r2)" -> in rb3(QTk/radiobutton) out -> in#3 lrradio()
"create(text:"choice 4" group:r2)" -> in rb4(QTk/radiobutton) out -> in#4 lrradio()

text(QTk/text) out -> in#12 td()
"create()" -> in text()

canvas(QTk/canvas) out -> in#13 td()
"create(width:500 height:500 bg:red)" -> in canvas()
"create(10 10 100 100)" -> in arc(QTk/canvas/arc) out -> widget canvas()